{
  "name": "multi-page",
  "version": "1.0.0",
  "title": "multi-page",
  "description": "webpack multi-page",
  "homepage": "https://biorz.github.io/boilerplate",
  "keywords": [
    "component"
  ],
  "main": "dist",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "dev": "webpack-dev-server --content-base test/ --open --inline --hot --compress --history-api-fallback --port 5050 --config build/webpack.dev.config.js",
    "dist:dev": "webpack --config build/webpack.dist.dev.config.js",
    "dist:prod": "webpack --config build/webpack.dist.prod.config.js",
    "dist": "npm run dist:dev && npm run dist:prod",
    "unit": "cross-env BABEL_ENV=test karma start test/unit/karma.conf.js --single-run",
    "test": "npm run lint && npm run unit",
    "pub": "npm run dist && node build/deploy.js",
    "build": "webpack --config build/webpack.dist.config.js"
  },
  "repository": {
